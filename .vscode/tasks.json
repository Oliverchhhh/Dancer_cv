			"options": {
				"cwd": "D://mingw64//bin"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"detail": "编译器:D://mingw64//bin//g++.exe"
